<?xml version="1.0" encoding="utf-8" ?>
<CiTable>
  <TableName>PatientDetail</TableName>
  <TableCaption>Patient Detail</TableCaption>
  <RowKey>FPatientNo,MPatientNo</RowKey>
  <DefaultView>Card</DefaultView>
  <CiTextField>
    <FieldName>FPatientNo</FieldName>
    <Hidden>true</Hidden>
  </CiTextField>
  <CiTextField>
    <FieldName>MPatientNo</FieldName>
    <Hidden>true</Hidden>
  </CiTextField>
  <SelectMacro>
    <ActionSQL>
      select 
        @FPatientNo as FPatientNo,
        @MPatientNo as MPatientNo
    </ActionSQL>
  </SelectMacro>
  <CiTable src="FProfile.pux" />
  <CiTable src="MProfile.pux" />
</CiTable>